		.wrapper
			// Navbar
			nav.main-header.navbar.navbar-expand.navbar-white.navbar-light
				// Left navbar links
				ul.navbar-nav
					li.nav-item
						a.nav-link(data-widget='pushmenu' href='#' role='button')
							i.fas.fa-bars
					li.nav-item
						li.nav-item.d-none.d-sm-inline-block
						a.nav-link(href='../') Trang chủ
				ul.navbar-nav.ml-auto
					li.nav-item
						li.nav-item.d-none.d-sm-inline-block
						a.nav-link(href='/logout') Logout
			// Main Sidebar Container
			aside.main-sidebar.sidebar-dark-primary.elevation-4
				// Brand Logo
				a.brand-link(href='#')
					img.brand-image.img-circle.elevation-3(src='/images/logo2.png' alt='AdminLTE Logo' style='opacity: .8')
