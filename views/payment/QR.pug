											a.nav-link(href='../')  Trang chủ
										li.nav-item
											a.nav-link(href='../intro') Giới thiệu
										li.nav-item
											a.nav-link(href='../contact') Liên hệ
										//- li.nav-item
										//- 	a.nav-link(href='./customer') Tài khoản
										//- if(session.user)
										if(session.user && session.user.type == '2')
											li.nav-item
												a.nav-link(href='../admin') Admin
										if(session.user)
											li.nav-item
												a.nav-link(href='../logout') Đăng xuất
										
									if(session.user)
										.sign_btn
											a(href='../customer') #{session.client.name}
									else 
										.sign_btn
											a(href='../login') Đăng nhập
								
										
									
